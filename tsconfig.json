{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "@prismaClient": [
        "./prisma/generated/client"
      ],
      "@prismaAuthClient": [
        "./prisma/generated/auth-client"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@UI/*": [
        "./pages/Components/UI/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "es2015",
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/login/index.jsx",
    "pages/register/index.jsx",
    ".next/types/**/*.ts"
, "components/user_bar/user_bar.jsx", "pages/user/[userId]/index.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}